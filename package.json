{
  "dependencies": {
    "commander": "^9.3.0",
    "execa": "^5.0.0"
  },
  "description": "Simple CLI for upload tf modules to Gitlab registry",
  "devDependencies": {
    "@types/node": "^18.0.4",
    "commitizen": "^4.2.4",
    "skuba": "*"
  },
  "files": [
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "lib*/**/*.json"
  ],
  "bin": {
    "gvnn-tf-publish": "lib/index.js"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "name": "@gvnn/tf-publish",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gvnn/gvnn-tf-publish.git"
  },
  "scripts": {
    "build": "skuba build && chmod +x 'lib/index.js'",
    "commit": "cz",
    "format": "skuba format",
    "lint": "skuba lint",
    "release": "yarn build && skuba release",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch",
    "start": "node lib/index.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "sideEffects": false,
  "types": "./lib-types/index.d.ts",
  "version": "0.0.0-semantically-released",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
